<!DOCTYPE html>
<html>
  <head>
    <title>Quiz2</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <body>
      <header class="row">
        <div class="col-3">
          <div class="logo">
            LOGO
            <br />
          </div>
        </div>
        <div class="col-4">
          Dad Jokes for the everyday dad.
        </div>
        <div class="col-5 links">
          <%= link_to 'Contribute', new_joke_path %>
           

        </div>
      </header>


      <br class="clear" />

      <div class="container">
        <% if flash[:error].present? %>
          <div class="col-10 offset-1 alert alert-danger">
            <%= flash[:error].html_safe %>
          </div>
        <% end %>
        


    <%= yield %>


  </body>
</html>
